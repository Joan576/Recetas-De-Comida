{"ast":null,"code":"import { ref } from 'vue';\nimport axios from 'axios'; // Importar Axios\n\nexport default {\n  __name: 'registerForm',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const form = ref({\n      username: '',\n      email: '',\n      password: '',\n      confirmPassword: ''\n    });\n    const error = ref('');\n    const handleSubmit = async () => {\n      // Validar que las contraseñas coincidan\n      if (form.value.password !== form.value.confirmPassword) {\n        error.value = 'Passwords do not match!';\n        return;\n      }\n\n      // Limpiar error si todo es válido\n      error.value = '';\n      try {\n        // Hacer la petición POST al backend\n        const response = await axios.post('localhost/4000', {\n          nombre_usuario: form.value.username,\n          correo: form.value.email,\n          contraseña: form.value.password\n        });\n\n        // Manejar la respuesta exitosa\n        console.log('User registered:', response.data);\n        // Puedes redirigir al usuario, mostrar un mensaje, etc.\n      } catch (err) {\n        // Manejar errores, como problemas con el servidor\n        console.error('Error registering user:', err);\n        error.value = 'Registration failed. Please try again.';\n      }\n    };\n    const __returned__ = {\n      form,\n      error,\n      handleSubmit,\n      ref,\n      get axios() {\n        return axios;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","axios","form","username","email","password","confirmPassword","error","handleSubmit","value","response","post","nombre_usuario","correo","contraseña","console","log","data","err"],"sources":["C:/Users/MEZAS/OneDrive/Escritorio/Trabajos U/Programacion Web/Proyecto_Recetas/recetas_front/src/components/registerForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"register-container\">\r\n    <h2>Register</h2>\r\n    <form @submit.prevent=\"handleSubmit\">\r\n      <div>\r\n        <label for=\"username\">Username:</label>\r\n        <input v-model=\"form.username\" type=\"text\" id=\"username\" required />\r\n      </div>\r\n\r\n      <div>\r\n        <label for=\"email\">Email:</label>\r\n        <input v-model=\"form.email\" type=\"email\" id=\"email\" required />\r\n      </div>\r\n\r\n      <div>\r\n        <label for=\"password\">Password:</label>\r\n        <input v-model=\"form.password\" type=\"password\" id=\"password\" required />\r\n      </div>\r\n\r\n      <div>\r\n        <label for=\"confirmPassword\">Confirm Password:</label>\r\n        <input v-model=\"form.confirmPassword\" type=\"password\" id=\"confirmPassword\" required />\r\n      </div>\r\n\r\n      <div v-if=\"error\" class=\"error\">{{ error }}</div>\r\n\r\n      <button type=\"submit\">Register</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref } from 'vue';\r\nimport axios from 'axios'; // Importar Axios\r\n\r\nconst form = ref({\r\n  username: '',\r\n  email: '',\r\n  password: '',\r\n  confirmPassword: '',\r\n});\r\n\r\nconst error = ref('');\r\n\r\nconst handleSubmit = async () => {\r\n  // Validar que las contraseñas coincidan\r\n  if (form.value.password !== form.value.confirmPassword) {\r\n    error.value = 'Passwords do not match!';\r\n    return;\r\n  }\r\n  \r\n  // Limpiar error si todo es válido\r\n  error.value = '';\r\n\r\n  try {\r\n    // Hacer la petición POST al backend\r\n    const response = await axios.post('localhost/4000', {\r\n      nombre_usuario: form.value.username,\r\n      correo: form.value.email,\r\n      contraseña: form.value.password,\r\n    });\r\n\r\n    // Manejar la respuesta exitosa\r\n    console.log('User registered:', response.data);\r\n    // Puedes redirigir al usuario, mostrar un mensaje, etc.\r\n  } catch (err) {\r\n    // Manejar errores, como problemas con el servidor\r\n    console.error('Error registering user:', err);\r\n    error.value = 'Registration failed. Please try again.';\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.register-container {\r\n  max-width: 400px;\r\n  margin: 0 auto;\r\n}\r\n.error {\r\n  color: red;\r\n}\r\n</style>\r\n"],"mappings":"AAgCA,SAASA,GAAG,QAAQ,KAAK;AACzB,OAAOC,KAAK,MAAM,OAAO,CAAC,CAAC;;;;;;;;IAE3B,MAAMC,IAAI,GAAGF,GAAG,CAAC;MACfG,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE;IACnB,CAAC,CAAC;IAEF,MAAMC,KAAK,GAAGP,GAAG,CAAC,EAAE,CAAC;IAErB,MAAMQ,YAAY,GAAG,MAAAA,CAAA,KAAY;MAC/B;MACA,IAAIN,IAAI,CAACO,KAAK,CAACJ,QAAQ,KAAKH,IAAI,CAACO,KAAK,CAACH,eAAe,EAAE;QACtDC,KAAK,CAACE,KAAK,GAAG,yBAAyB;QACvC;MACF;;MAEA;MACAF,KAAK,CAACE,KAAK,GAAG,EAAE;MAEhB,IAAI;QACF;QACA,MAAMC,QAAQ,GAAG,MAAMT,KAAK,CAACU,IAAI,CAAC,gBAAgB,EAAE;UAClDC,cAAc,EAAEV,IAAI,CAACO,KAAK,CAACN,QAAQ;UACnCU,MAAM,EAAEX,IAAI,CAACO,KAAK,CAACL,KAAK;UACxBU,UAAU,EAAEZ,IAAI,CAACO,KAAK,CAACJ;QACzB,CAAC,CAAC;;QAEF;QACAU,OAAO,CAACC,GAAG,CAAC,kBAAkB,EAAEN,QAAQ,CAACO,IAAI,CAAC;QAC9C;MACF,CAAC,CAAC,OAAOC,GAAG,EAAE;QACZ;QACAH,OAAO,CAACR,KAAK,CAAC,yBAAyB,EAAEW,GAAG,CAAC;QAC7CX,KAAK,CAACE,KAAK,GAAG,wCAAwC;MACxD;IACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}